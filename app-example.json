{
  "expo": {
    "name": "zhvelopumpi",
    "description": "Finde öffentliche Zürcher Velopumpstation in einer einfach zu bedienenden App.",
    "githubUrl":"https://github.com/booooza/zh-velo-pumpi",
    "slug": "zhvelopumpi",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#268BCC"
    },
    "primaryColor":"#268BCC",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcbischof.zhvelopumpi"
    },
    "android": {
      "package": "com.marcbischof.zhvelopumpi",
      "config": {
        "googleMaps": {
          "apiKey": "API Key"
        }
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
